{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "txHash",
    "signature"
  ],
  "properties": {
    "txHash": {
      "$id": "#/properties/txHash",
      "type": "string",
      "title": "The Txhash Schema",
      "default": "",
      "examples": [
        "OriginalTx commit hash"
      ],
      "pattern": "^(.*)$"
    },
    "signature": {
      "$id": "#/properties/signature",
      "type": "string",
      "title": "The Signature Schema",
      "default": "",
      "examples": [
        "RecipientSignature"
      ],
      "pattern": "^(.*)$"
    }
  }
}